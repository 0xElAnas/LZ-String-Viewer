{
  "manifest_version": 3,
  "name": "LZ-String Viewer",
  "version": "1.0",
  "description": "Read and decompress localStorage and sessionStorage data compressed with lz-string.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "toolbar.html"
  },
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "devtools_page": "devtools.html"
}
