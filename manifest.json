{
  "manifest_version": 3,
  "name": "LZ-String LocalStorage Reader",
  "version": "1.0",
  "description": "Read and decompress localStorage data compressed with lz-string.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "devtools_page": "devtools.html"
}
